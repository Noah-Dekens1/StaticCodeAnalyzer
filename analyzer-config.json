{
  "analyzers": [
    {
      "class_parents": {
        "enabled": true,
        "max_parents": 10
      },
      "large_methods": {
        "enabled": true,
        "max_statements": 70
      },
      "if_else": {
        "enabled": true,
        "max_elses": 7
      },
      "large_types": {
        "enabled": true,
        "max_members": 50
      },
      "magic_numbers": {
        "enabled": false,
        "severity": "suggestion"
      },
      "method_parameter_count": {
        "enabled": true,
        "max_parameters": 5
      },
      "nested_ternary": {
        "enabled": true,
        "severity": "important"
      },
      "partial_variable_assignment": {
        "enabled": true
      },
      "switch_cases": {
        "enabled": true,
        "max_cases": 20
      },
      "test_assertions": {
        "enabled": true,
        "any_name_including_assert": true,
        "check_called_methods": false,
        "use_custom_assertion_methods": true,
        "assertion_methods": [
          "ValidateTokens",
          "ValidateTokenPositions"
        ]
      },
      "unused_parameters": {
        "enabled": true,
        "ignore_when_implementing_types": [

        ]
      }
    }
  ],
  "severities": {
    "suggestion": 0,
    "warning": 1,
    "important": 2
  },
  "code_guard": {
    "fail_on_reach_severity_score": true,
    "max_allowed_severity_score": 50
  },
  "directories": {
    "excluded": [
      "/**/Migrations/*.cs",
      "/**/obj/**/*"
    ]
  }
}